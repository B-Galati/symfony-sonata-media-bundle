{
  "name": "mediamonks/sonata-media-bundle",
  "type": "symfony-bundle",
  "description": "A powerful alternative, flexible and easy to use alternative for the existing Sonata Media Bundle",
  "keywords": [
    "media",
    "image",
    "video",
    "sound",
    "embed",
    "oembed",
    "youtube",
    "bundle",
    "symfony",
    "sonata",
    "admin"
  ],
  "homepage": "https://www.mediamonks.com/",
  "license": "MIT",
  "authors": [
    {
      "name": "Robert Slootjes",
      "email": "robert@mediamonks.com",
      "homepage": "https://github.com/slootjes"
    }
  ],
  "require": {
    "php": "^5.5|^7.0",
    "symfony/dependency-injection": "^2.8|^3.0",
    "symfony/http-kernel": "^2.8|^3.0",
    "league/flysystem": "^1.0",
    "league/glide": "^1.2"
  },
  "require-dev": {
    "sensio/framework-extra-bundle": "^3.0",
    "matthiasnoback/symfony-dependency-injection-test": "0.*",
    "mockery/mockery": "^0.9.4",
    "oneup/flysystem-bundle": "^1.13",
    "doctrine/doctrine-bundle": "^1.6",
    "sonata-project/admin-bundle": "~3.0",
    "sonata-project/doctrine-orm-admin-bundle": "~3.0",
    "liip/functional-test-bundle": "^1.7",
    "doctrine/doctrine-fixtures-bundle": "^2.3",
    "symfony/symfony": "~2.8.0",
    "mikey179/vfsStream": "^1.6",
    "php-vcr/php-vcr": "dev-master@dev",
    "phpunit/phpunit": "~4.8"
  },
  "suggest": {
    "oneup/flysystem-bundle": "Makes the Flysystem configuration a lot easier"
  },
  "autoload": {
    "psr-4": {
      "MediaMonks\\SonataMediaBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MediaMonks\\SonataMediaBundle\\Tests\\": "tests/"
    },
    "psr-0": {
      "": "tests/Functional/src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
